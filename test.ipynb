    "import requests\n",
    "\n",
    "url = \"http://localhost:8000/items/99\"\n",
    "data = {\n",
    "    \"name\": \"Tablet\",\n",
    "    \"price\": 399.0,\n",
    "    \"is_offer\": None\n",
    "}\n",
    "\n",
    "response = requests.put(url, json=data)\n",
    "print(\"Status Code:\", response.status_code)\n",
    "print(\"Response JSON:\", response.json())"
   ]
  }
